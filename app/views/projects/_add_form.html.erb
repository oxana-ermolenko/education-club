<%= form_for @project do |form| %>
  <div class="field">
    <%= form.label :title %><br>
    <%= form.text_field :title %>
    <% @project.errors.full_messages_for(:title).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  


  <div class="field">
    <%= form.submit "Submit", class: "ui submit basic button positive " %>
  </div>
<% end %>