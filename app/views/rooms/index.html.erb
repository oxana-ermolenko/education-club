<div class="ui grid custom_margin">
  <div class="two wide column">
    <div class="ui vertical fluid tabular menu">
      <%= link_to projects_path, class: "item" do %>
        <i class="file altenate icon"></i> Profile
      <% end %>
      <%= link_to rooms_path, class: "active item" do %>
        <i class="comment alternate icon"></i> Messenger
      <% end %>
      <a class="item">
        My Messenger
      </a>
      <%= link_to calendar_path, class: "item" do %>
        <i class="columns icon"></i> My Calendar
      <% end %>
    </div>
  </div>
  <div class="twelve wide stretched column">
    <div class="ui segment">
      <div class="ui grid">

        <div class="four wide column">
          <button class="ui positive basic btnModalNewRoom button">
            <i class="comment alternate icon"></i>New
          </button>
        </div>
      </div>
      <div class="ui grid">
        <%= render 'room', obj: @rooms %>
      </div>
      <div class="ui two column centered grid custom_margin_bottom">
      <%= will_paginate @rooms, renderer: WillPaginateSemanticUi::ActionView::Renderer %>
      </div>
    </div>
 </div>
 <div class="mb"></div>
 
 <div class="ui test modal ">
 <i class="close icon"></i>
 <div class="ui row">
 <div class="ui four wide column segment">
   <h2 class="ui center aligned dividing header">New Room</h2>
   <div class="ui form ">
    <%= render "form" %>
   </div>
 </div>
</div>  

</div>




      